  <script>
    function redirectToWhatsApp() {
      const phoneNumber = '+918608315342';
      const message = 'Hello! I\'m interested in BMW vehicles and would like to get expert advice. Can you please help me with information about your latest models, pricing, and available features?';
      const whatsappUrl = `https://wa.me/${phoneNumber}?text=${encodeURIComponent(message)}`;
      window.open(whatsappUrl, '_blank');
    }

    // Prevent auto-scrolling on page load
    window.addEventListener('load', function() {
      // Remove any hash from URL to prevent auto-scroll
      if (window.location.hash) {
        history.replaceState(null, null, window.location.pathname);
      }
      
      // Force scroll to top of page
      window.scrollTo(0, 0);
      
      // Additional prevention for any auto-scroll
      setTimeout(function() {
        window.scrollTo(0, 0);
      }, 100);
      
      setTimeout(function() {
        window.scrollTo(0, 0);
      }, 500);
    });

    // Also prevent on DOMContentLoaded
    document.addEventListener('DOMContentLoaded', function() {
      // Remove any hash from URL
      if (window.location.hash) {
        history.replaceState(null, null, window.location.pathname);
      }
      
      // Force scroll to top
      window.scrollTo(0, 0);
      
      // Prevent any automatic scrolling
      document.body.style.scrollBehavior = 'auto';
      
      // Override any scrollIntoView behavior
      const originalScrollIntoView = Element.prototype.scrollIntoView;
      Element.prototype.scrollIntoView = function() {
        // Only allow manual scrolling, not automatic
        if (arguments.length > 0 && arguments[0].behavior === 'smooth') {
          return originalScrollIntoView.apply(this, arguments);
        }
        return false;
      };
    });

    // Prevent page navigation on refresh
    window.addEventListener('beforeunload', function(e) {
      // Clear any stored scroll position
      sessionStorage.removeItem('scrollPosition');
    });

    // Ensure page stays on current URL
    window.addEventListener('pageshow', function(event) {
      // If page is loaded from cache, ensure we're on the correct page
      if (event.persisted) {
        window.location.reload();
      }
    });

    // Prevent form submissions from navigating away
    document.addEventListener('submit', function(e) {
      e.preventDefault();
      return false;
    });

    // Prevent all links from navigating away (except external links)
    document.addEventListener('click', function(e) {
      const target = e.target.closest('a');
      if (target) {
        const href = target.getAttribute('href');
        // Allow external links and phone/email links
        if (href && (href.startsWith('http') || href.startsWith('tel:') || href.startsWith('mailto:') || href.startsWith('#'))) {
          return true; // Allow these links
        }
        // Prevent internal navigation
        e.preventDefault();
        return false;
      }
    });

    // Smooth scrolling for navigation links
    document.addEventListener('DOMContentLoaded', function() {
      const smoothScrollLinks = document.querySelectorAll('.smooth-scroll');
      smoothScrollLinks.forEach(link => {
        link.addEventListener('click', function(e) {
          e.preventDefault();
          const targetId = this.getAttribute('href').substring(1);
          const targetElement = document.getElementById(targetId);
          if (targetElement) {
            targetElement.scrollIntoView({
              behavior: 'smooth',
              block: 'start'
            });
          }
        });
      });
      
      // Additional prevention for any auto-scroll to shopping section
      const shoppingSection = document.getElementById('shopping');
      if (shoppingSection) {
        // Remove any focus or scroll triggers
        shoppingSection.removeAttribute('tabindex');
        shoppingSection.style.scrollMarginTop = '0';
      }
    });
  </script>
